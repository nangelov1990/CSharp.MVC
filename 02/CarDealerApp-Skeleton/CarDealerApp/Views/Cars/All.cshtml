@model IEnumerable<CarDealer.Models.ViewModels.AllCarsByMakeVm>

@{
    ViewBag.Title = "All";
}

<h2>All</h2>

<table class="table table-striped">
    <tr>
        <th>Make</th>
        <th>Model</th>
        <th>TravelledDistance</th>
        <th></th>
    </tr>
    @foreach (var item in Model) {
    <tr>
        <td>@item.Make</td>
        <td>@item.Model</td>
        <td>@item.TravelledDistance</td>
    </tr>
}
</table>
