@model IEnumerable<CarDealer.Models.ViewModels.Suppliers.AllSuppliersVm>

@{
    ViewBag.Title = "All";
}

<h2>All</h2>

<table class="table table-striped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Number Of Parts To Supply</th>
    </tr>
    @foreach (var item in Model) {
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.NumberOfPartsToSupply</td>
    </tr>
}

</table>
